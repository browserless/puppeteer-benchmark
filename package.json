{
  "name": "puppeteer-test-tool",
  "version": "1.0.0",
  "description": "",
  "bin": {
    "puppeteer-test": "./build/cli.js"
  },
  "main": "build",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "make-puppeteer-placeholder": "node ./build/helpers/make-puppeteer-placeholder.js",
    "postinstall": "npm run build && npm run make-puppeteer-placeholder",
    "prepare": "npx simple-git-hooks",
    "watch": "ts-node-dev --transpile-only"
  },
  "dependencies": {
    "commander": "^9.4.1",
    "mathjs": "^11.3.3",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "lint-staged": "^13.0.3",
    "rewiremock": "^3.14.4",
    "rome": "^10.0.1",
    "simple-git-hooks": "^2.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{ts}": [
      "rome format --write"
    ]
  }
}
